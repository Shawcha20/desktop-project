<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="639.0" prefWidth="484.0" style="-fx-background-color: #000;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login_signin_signup_javafx.signInController">
   <children>
      <ImageView fitHeight="497.0" fitWidth="461.0" layoutX="21.0" layoutY="54.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@aivshuman.png" />
         </image>
      </ImageView>
      <Button fx:id="btn_gotoLogin" layoutX="199.0" layoutY="590.0" mnemonicParsing="false" onAction="#goto_login_page" text="Log Out">
         <font>
            <Font name="Berlin Sans FB" size="18.0" />
         </font></Button>
      <CheckBox fx:id="human" layoutX="313.0" layoutY="551.0" mnemonicParsing="false" onAction="#humanScene" text="Human" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="18.0" />
         </font></CheckBox>
      <CheckBox fx:id="computer" layoutX="71.0" layoutY="551.0" mnemonicParsing="false" onAction="#aiComputer" text="computer" textFill="#fffdfd">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="18.0" />
         </font></CheckBox>
      <Label layoutX="146.0" layoutY="14.0" style="-fx-background-color: #ffff;" text="Choose A Game Mode">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
